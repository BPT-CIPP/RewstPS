            "fields": {
                "body": "$Request = \"{{ CTX.request}}\"\n$Base64Signature = [System.Convert]::ToBase64String([System.Text.Encoding]::UTF8.GetBytes($Request))\n$Hash = [System.Security.Cryptography.HashAlgorithm]::Create('sha256').ComputeHash([System.Text.Encoding]::UTF8.GetBytes($Base64Signature))\n([System.BitConverter]::ToString($Hash).Replace('-', '').ToLower())",
                "contentType": "script",
                "language": "powershell"
            },
            "nonfunctional_fields": {
                "description": "Takes a string and returns it as a sha256 hash. Requires a string in CTX.request.",
